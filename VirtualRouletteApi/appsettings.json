{
  "Storage": {
    "Provider": "Postgres"
  },
  "ConnectionStrings": {
    "Postgres": "Host=localhost;Port=5432;Database=vr;Username=postgres;Password=postgres",
    "Redis": "localhost:6379"
  },
  "Auth": {
    "Mode": "Jwt",
    "Basic": {
      "Realm": "VirtualRouletteApi"
    },
    "Jwt": {
      "Issuer": "VirtualRouletteApi",
      "Audience": "VirtualRouletteApiClient",
      "SigningKey": "str",
      "ExpiresMinutes": 60
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
